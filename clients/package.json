{
  "name": "i2c",
  "version": "1.0.0",
  "description": "I2C Bus Client Samples",
  "author": "David Kebler <d@kebler.net>",
  "license": "MIT",
  "scripts": {
    "scan": "BUS_HOST=sbc node --require esm scan",
    "relay": "BUS_HOST=sbc I2C_ADDRESS=39 node --require esm relay",
    "switch": "node --require esm mcp-switch",
    "switch2": "node --require esm mcp-switch2",
    "mrelay": "BUS_HOST=sbc node --require esm mcp-relay"
  },
  "dependencies": {
    "@uci/base": "^0.1.8",
    "@uci/mcp": "^0.1.8",
    "@uci/socket": "^0.1.9"
  },
  "devDependencies": {
    "esm": "^3.0.38"
  }
}
